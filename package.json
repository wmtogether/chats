{
  "name": "www",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"cargo run --bin mikochat\" \"cd Server && go run . \" ",
    "build": "vite build && cargo build --release && bun run Library/Shared/Scripts/postbuild.ts",
    "postbuild": "bun run Library/Shared/Scripts/postbuild.ts",
    "build-installer": "bun run Library/Shared/Scripts/build-installer.ts",
    "build-all": "bun run build && bun run build-installer",
    "typecheck": "tsc --noEmit",
    "test": "bun run typecheck && cargo test --manifest-path Desktop/Cargo.toml",
    "preview": "vite preview",
    "preview:client": "vite build && cargo run --release --bin workspace"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "typescript": "^5",
    "vite": "^7.3.0",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "axios": "^1.13.4",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "emoji-picker-react": "^4.16.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.562.0",
    "process": "^0.11.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.13.0",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "util": "^0.12.5"
  }
}
